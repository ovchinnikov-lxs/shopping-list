{
    "name": "@ovchinnikov-lxs/shopping-list",
    "version": "2.0.0",
    "scripts": {
        "build": "pnpm run generate-types && nuxt build",
        "dev": "pnpm run generate-types && nuxt dev --host",
        "generate": "pnpm run generate-types && nuxt generate",
        "preview": "pnpm run generate-types && nuxt preview",
        "postinstall": "pnpm run generate-types && nuxt prepare",
        "prepare": "husky install",
        "el:check": "eslint --ext .ts --ext .vue .",
        "sl:check": "stylelint \"**/*.{css,scss,vue}\"",
        "lint:check": "npm run el:check && npm run sl:check",
        "el:fix": "eslint --ext .vue --ext .ts . --fix",
        "sl:fix": "stylelint \"**/*.{css,scss,vue}\" --fix",
        "lint:fix": "npm run el:fix && npm run sl:fix",
        "generate-types": "npx supabase gen types typescript --project-id \"rihjywbwdijfaikghqkm\" > types/supabase.ts"
    },
    "dependencies": {
        "@nuxtjs/supabase": "^1.1.6",
        "@ovchinnikov-lxs-frontend/ui-kit": "^2.10.3",
        "@pinia/nuxt": "^0.4.11",
        "lodash": "^4.17.21",
        "nanoid": "^5.0.5",
        "normalize.css": "^8.0.1",
        "pinia": "^2.1.6",
        "qrcode": "^1.5.3",
        "supabase": "^1.142.2"
    },
    "devDependencies": {
        "@nuxt/devtools": "latest",
        "@nuxtjs/eslint-config-typescript": "^12.0.0",
        "@nuxtjs/i18n": "^8.1.1",
        "@types/lodash": "^4.14.202",
        "@types/node": "^18.16.19",
        "@types/qrcode": "^1.5.2",
        "@vite-pwa/nuxt": "^0.4.0",
        "eslint": "^8.34.0",
        "husky": "^8.0.3",
        "nano-staged": "^0.5.0",
        "nuxt": "^3.10.2",
        "sass": "^1.58.0",
        "stylelint": "^13.13.1",
        "stylelint-config-css-modules": "^2.2.0",
        "stylelint-config-standard": "^20.0.0",
        "stylelint-order": "^4.1.0",
        "stylelint-scss": "^3.14.2",
        "vite-plugin-stylelint": "^4.2.0",
        "vue-tsc": "^1.6.5"
    },
    "overrides": {
        "vue": "latest"
    }
}
